getHtml	Python\downloadImg.py	/^def getHtml(url):$/;"	f
getImg	Python\downloadImg.py	/^def getImg(html):$/;"	f
html	Python\downloadImg.py	/^html = getHtml("http:\/\/www.ortesj.cn\/")$/;"	v
html	Python\downloadImg.py	/^html = page.read()$/;"	v
imglist	Python\downloadImg.py	/^imglist = re.findall(imgre,html)$/;"	v
imgre	Python\downloadImg.py	/^imgre = re.compile(reg)$/;"	v
page	Python\downloadImg.py	/^page = urllib.urlopen(url)$/;"	v
re	Python\downloadImg.py	/^import re$/;"	i
urllib	Python\downloadImg.py	/^import urllib$/;"	i
x	Python\downloadImg.py	/^x=0$/;"	v
